{
  "name": "cybersage-v2-frontend",
  "version": "2.0.0",
  "private": true,
  "dependencies": {
    "@headlessui/react": "^1.7.17",
    "@tanstack/react-query": "^5.8.4",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "chart.js": "^4.4.0",
    "clsx": "^2.0.0",
    "date-fns": "^2.30.0",
    "file-saver": "^2.0.5",
    "framer-motion": "^10.16.4",
    "lucide-react": "^0.263.1",
    "react": "^18.2.0",
    "react-chartjs-2": "^5.2.0",
    "react-circular-progressbar": "^2.1.0",
    "react-dom": "^18.2.0",
    "react-hot-toast": "^2.4.1",
    "react-intersection-observer": "^9.5.3",
    "react-joyride": "^2.5.5",
    "react-router-dom": "^6.20.1",
    "react-scripts": "5.0.1",
    "react-syntax-highlighter": "^15.5.0",
    "react-virtualized-auto-sizer": "^1.0.26",
    "react-window": "^1.8.10",
    "react-window-infinite-loader": "^2.0.0",
    "recharts": "^3.3.0",
    "socket.io-client": "^4.7.2",
    "web-vitals": "^2.1.4",
    "workbox-webpack-plugin": "^7.3.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "SKIP_PREFLIGHT_CHECK=true react-scripts build",
    "test": "react-scripts test",
    "test:coverage": "react-scripts test --coverage --watchAll=false",
    "test:e2e": "cypress run",
    "test:e2e:open": "cypress open",
    "test:accessibility": "cypress run --spec 'cypress/e2e/accessibility.cy.js'",
    "test:performance": "cypress run --spec 'cypress/e2e/performance.cy.js'",
    "test:all": "npm run test:coverage && npm run test:e2e && npm run test:accessibility",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "plugin:jsx-a11y/recommended"
    ],
    "plugins": ["jsx-a11y"],
    "rules": {
      "jsx-a11y/anchor-is-valid": 0,
      "jsx-a11y/click-events-have-key-events": 1,
      "jsx-a11y/no-static-element-interactions": 1
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "autoprefixer": "^10.4.17",
    "cypress": "^13.6.0",
    "cypress-axe": "^1.5.0",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "jsdom": "^23.0.0",
    "postcss": "^8.4.33",
    "react-scripts": "5.0.1",
    "tailwindcss": "^3.4.0"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!src/index.js",
      "!src/reportWebVitals.js",
      "!src/setupTests.js"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 80,
        "functions": 80,
        "lines": 80,
        "statements": 80
      }
    }
  }
}